<!--Homepage-->
<div class="container">
    <div class="col s12">
        <div class="text-center">
            <h1> New York Times Scraper</h1>
        </div>

    </div>
    <div class="col s12">
        {{#if article}}
            {{#each article}}
            <div>
                <div>
                    <h4>{{this.title}}</h4>
                </div>
                <div >
                    <p> {{this.summary}}</p>
                    <a href={{this.link}} target="_blank">{{this.link}}</a>
                    <br><br>
                    <a class="#1de9b6 teal accent-4 waves-effect waves-light btn save"data-id={{this._id}}>Save Article</a>

                </div>
            </div>
            {{/each}}
      
            </div>
         </div>
        {{/if}}
    </div>
</div>